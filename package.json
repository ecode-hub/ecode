{
  "name": "ecode",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "analyze": "webpack-dev-server --config webpack.analyze.config.js",
    "dev": "npm run start",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "eslint 'src/**/*.{tsx,ts}'",
    "lint:style": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:ts:fix": "eslint 'src/**/*.{tsx,ts}' --fix",
    "lint:style:fix": "stylelint 'src/**/*.scss' --syntax scss --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": "eslint",
    "src/**/*.scss": "stylelint"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.10",
    "@types/eslint": "^6.1.3",
    "@types/highlightjs": "^9.12.0",
    "@types/lodash": "^4.14.150",
    "@types/markdown-it": "^10.0.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.4",
    "@types/simplemde": "^1.11.7",
    "@types/webpack": "^4.41.2",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react-hooks": "^0.0.0-d7382b6c4",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "stylelint": "^13.5.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.7.4",
    "url-loader": "^4.1.0",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "antd": "^4.6.3",
    "axios": "^0.19.2",
    "highlight.js": "^10.0.0",
    "image-conversion": "^2.1.1",
    "lodash": "^4.17.15",
    "markdown-it": "^10.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.1.2",
    "react-use": "^14.1.1",
    "simplemde": "^1.11.2"
